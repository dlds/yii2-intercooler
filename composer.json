{
    "name": "dlds/yii2-intercooler",
    "description": "Yii2 Intercooler extensions",
    "keywords": ["yii2", "intercooler"],
    "type": "yii2-extension",
    "license": "BSD",
    "support": {
        "issues": "https://github.com/dlds/yii2-intercooler/issues",
        "wiki": "https://github.com/dlds/yii2-intercooler/wiki",
        "source": "https://github.com/dlds/yii2-intercooler"
    },
    "authors": [
        {
            "name": "Jiri Svoboda",
            "email": "jiri.svoboda@dlds.cz"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "bower-asset/intercooler-js": "~0.9.4"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-faker": "*",
        "codeception/specify": "*",
        "codeception/verify": "*"
    },
    "dependencies": {
        "intercooler-js": "0.9.4"
    },
    "autoload": {
        "psr-4": {
            "dlds\\intercooler\\": "src"
        }
    }
